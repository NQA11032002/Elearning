{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"../../assets/styles/login.scss\";\nimport axios from 'axios';\nimport Cookies from 'js-cookie';\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const router = useRouter();\n    if (Cookies.get('auth') !== undefined) {\n      router.push('/home');\n    }\n  },\n  data() {\n    return {\n      userName: '',\n      password: '',\n      isLogin: false\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        // Gửi yêu cầu POST đến API Spring Boot để đăng nhập\n        const response = await axios.post('http://localhost:8087/auth/login', {\n          userName: this.userName,\n          password: this.password\n        });\n\n        // Xử lý dữ liệu nhận được từ API (response.data) nếu cần\n        console.log('Login successful', response);\n        if (response.status == 200 && Cookies.get('auth') === undefined) {\n          Cookies.set('auth', response.data.token, {\n            expires: 1\n          }); // Expires in 7 days\n        }\n\n        this.$router.push('/home'); // Chuyển hướng đến trang home\n      } catch (error) {\n        // Xử lý lỗi từ API (error.response.data) nếu cần\n        console.error('Login failed', error.response.data);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","Cookies","useRouter","setup","router","get","undefined","push","data","userName","password","isLogin","methods","login","response","post","console","log","status","set","token","expires","$router","error"],"sources":["F:\\VueJS\\Elearning-Frontend\\Elearning\\src\\components\\client\\LoginScreen.vue"],"sourcesContent":["<template>\r\n    <body class=\"LoginForm\">\r\n        <div class=\"LoginForm_container\" id=\"container\">\r\n            <div class=\"form-container log-in-container\">\r\n                <form @submit.prevent=\"login\">\r\n                    <h1>Login</h1>\r\n                    <div class=\"social-container\">\r\n                        <a href=\"#\" class=\"social\"><i class=\"icon-fb fa-brands fa-facebook\"></i></a>\r\n                        <a href=\"#\" class=\"social\"><i class=\"icon-git fa-brands fa-github\"></i></a>\r\n                    </div>\r\n                    <span>or use your account</span>\r\n                    <input v-model=\"userName\" type=\"text\" placeholder=\"Username\" />\r\n                    <input v-model=\"password\" type=\"password\" placeholder=\"Password\" />\r\n                    <p class=\"text-red-500 font-semibold py-2 text-sm\">Tài khoản hoặc mật khẩu không chính xác</p>\r\n                    <a href=\"#\">Forgot your password?</a>\r\n                    <button type=\"submit\">Log In</button>\r\n                </form>\r\n            </div>\r\n            <div class=\"overlay-container\">\r\n                <div class=\"overlay\">\r\n                    <div class=\"overlay-panel overlay-right\">\r\n                        <h1>Login Form</h1>\r\n                        <p>Welcome to our website! Enjoy your time</p>\r\n                        <router-link to=\"/register\">\r\n                            <button>No account yet? Register</button>\r\n                        </router-link>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </body>\r\n</template>\r\n\r\n<script>\r\nimport \"../../assets/styles/login.scss\";\r\nimport axios from 'axios';\r\nimport Cookies from 'js-cookie';\r\nimport { useRouter } from 'vue-router';\r\n\r\nexport default {\r\n    setup() {\r\n        const router = useRouter();\r\n\r\n        if (Cookies.get('auth') !== undefined) {\r\n            router.push('/home');\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            userName: '',\r\n            password: '',\r\n            isLogin: false,\r\n            \r\n        };\r\n    },\r\n    methods: {\r\n        async login() {\r\n            try {\r\n                // Gửi yêu cầu POST đến API Spring Boot để đăng nhập\r\n                const response = await axios.post('http://localhost:8087/auth/login', {\r\n                    userName: this.userName,\r\n                    password: this.password,\r\n                });\r\n\r\n                // Xử lý dữ liệu nhận được từ API (response.data) nếu cần\r\n                console.log('Login successful', response);\r\n\r\n                if(response.status == 200 && Cookies.get('auth') === undefined){\r\n                    Cookies.set('auth', response.data.token, { expires: 1 }); // Expires in 7 days\r\n                }\r\n                this.$router.push('/home'); // Chuyển hướng đến trang home\r\n            } catch (error) {\r\n                // Xử lý lỗi từ API (error.response.data) nếu cần\r\n                console.error('Login failed', error.response.data);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n"],"mappings":";AAkCA,OAAO,gCAAgC;AACvC,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAC/B,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,MAAK,GAAIF,SAAS,CAAC,CAAC;IAE1B,IAAID,OAAO,CAACI,GAAG,CAAC,MAAM,MAAMC,SAAS,EAAE;MACnCF,MAAM,CAACG,IAAI,CAAC,OAAO,CAAC;IACxB;EACJ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IAEb,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,KAAKA,CAAA,EAAG;MACV,IAAI;QACA;QACA,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,kCAAkC,EAAE;UAClEN,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;;QAEF;QACAM,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAEH,QAAQ,CAAC;QAEzC,IAAGA,QAAQ,CAACI,MAAK,IAAK,GAAE,IAAKjB,OAAO,CAACI,GAAG,CAAC,MAAM,MAAMC,SAAS,EAAC;UAC3DL,OAAO,CAACkB,GAAG,CAAC,MAAM,EAAEL,QAAQ,CAACN,IAAI,CAACY,KAAK,EAAE;YAAEC,OAAO,EAAE;UAAE,CAAC,CAAC,EAAE;QAC9D;;QACA,IAAI,CAACC,OAAO,CAACf,IAAI,CAAC,OAAO,CAAC,EAAE;MAChC,EAAE,OAAOgB,KAAK,EAAE;QACZ;QACAP,OAAO,CAACO,KAAK,CAAC,cAAc,EAAEA,KAAK,CAACT,QAAQ,CAACN,IAAI,CAAC;MACtD;IACJ;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]