{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Cookies from 'js-cookie';\nimport axios from 'axios';\nexport default {\n  name: 'HeaderScreen',\n  data() {\n    return {\n      isAuthenticated: false,\n      showDropdown: false\n    };\n  },\n  mounted() {\n    this.checkAuthentication();\n  },\n  methods: {\n    checkAuthentication() {\n      const token = Cookies.get(\"auth\"); // Thay \"auth\" bằng tên của cookie lưu token\n\n      if (token) {\n        // Nếu có token, tức là đã đăng nhập\n        this.isAuthenticated = true;\n      } else {\n        // Nếu không có token, tức là chưa đăng nhập\n        this.isAuthenticated = false;\n      }\n    },\n    toggleDropdown() {\n      this.showDropdown = !this.showDropdown;\n    },\n    async logout() {\n      try {\n        const token = Cookies.get(\"auth\");\n        const formData = new FormData();\n        formData.append('token', token);\n        const response = await axios.post('http://localhost:8086/auth/logout', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        console.log(response);\n        this.deleteCookie(\"auth\");\n        localStorage.removeItem(\"idUser\");\n        this.$router.push('/login'); // Chuyển hướng đến trang home\n      } catch (error) {\n        console.error('Get infor failed', error.response.data);\n      }\n    },\n    deleteCookie(name) {\n      document.cookie = `${name}=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;`;\n    }\n  }\n};","map":{"version":3,"names":["Cookies","axios","name","data","isAuthenticated","showDropdown","mounted","checkAuthentication","methods","token","get","toggleDropdown","logout","formData","FormData","append","response","post","headers","console","log","deleteCookie","localStorage","removeItem","$router","push","error","document","cookie"],"sources":["F:\\VueJS\\Elearning-Frontend\\Elearning\\src\\components\\client\\HeaderScreen.vue"],"sourcesContent":["<template>\r\n  <header class=\"bg-white z-10 fixed top-0 flex items-center justify-between p-5 h-16 shadow-md shadow-gray-300 w-full left-0 right-0\">\r\n    <div class=\"flex items-center\">\r\n      <div class=\"header__logo\">\r\n        <img src=\"../../assets/images/client/logo.png\" alt=\"\">\r\n      </div>\r\n        <form action=\"\" class=\"relative border rounded-md px-2 flex items-center\">\r\n          <input type=\"text\" placeholder=\"Tìm khóa học, chuyên gia...\" class=\"left-0 outline-none h-full py-2 text-sm w-56\">\r\n          <label for=\"search__submit\" class=\"absolute right-3 cursor-pointer\">\r\n            <i class=\"fa-solid fa-magnifying-glass text-gray-400\"></i>\r\n          </label>\r\n          <input type=\"submit\" id=\"search__submit\" hidden value=\"tìm\">\r\n        </form>\r\n    </div>\r\n\r\n    <div class=\"header__middle\">\r\n      <ul class=\"flex justify-between\">\r\n        <li><a href=\"/home\" class=\"navigate__link mr-8\">Trang chủ</a></li>\r\n        <li><a href=\"/training\" class=\"navigate__link mr-8\">Đào tạo</a></li>\r\n        <li><a href=\"/course\" class=\"navigate__link mr-8\">Khóa học</a></li>\r\n        <li><a href=\"/support\" class=\"navigate__link mr-8\">Hỗ trợ</a></li>\r\n      </ul>\r\n    </div>\r\n\r\n    <div class=\"\">\r\n        <div v-if=\"isAuthenticated\" class=\"\">\r\n          <div  class=\"flex items-center gap-4 \">\r\n            <a href=\"/my-course\" class=\"font-medium border border-blue-900 text-blue-900  py-1 px-4 rounded-md transition-all hover:bg-blue-900 hover:text-white\">Học tập</a>\r\n            <div class=\"relative\">\r\n              <img @click=\"toggleDropdown\" class=\"w-8 h-8 rounded-full cursor-pointer\" src=\"../../assets/images/client/avatars/avatar.png\" alt=\"\">\r\n              <ul v-if=\"showDropdown\" class=\"absolute right-0 mt-2 bg-white border rounded-md shadow-md w-48 p-4\">\r\n                <li class=\"border-b border-gray-300 mb-2\"><a class=\"p-2\" href=\"/profile/1\">Thông tin tài khoản</a></li>\r\n                <li class=\"border-b border-gray-300 mb-2\"><a class=\"p-2\" href=\"/changepass/1\">Đổi mật khẩu</a></li>\r\n                <li><a class=\"p-2\" @click=\"logout\" href=\"#\">Đăng xuất</a></li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n          \r\n        </div>\r\n        \r\n        <div v-else class=\"flex items-center gap-4\">\r\n          <a href=\"/login\" class=\"font-medium border border-blue-900 text-blue-900  py-1 px-4 rounded-md transition-all hover:bg-blue-900 hover:text-white\">Đăng nhập</a>\r\n          <a href=\"/register\" class=\"font-medium border border-blue-900 text-blue-900  py-1 px-4 rounded-md transition-all hover:bg-blue-900 hover:text-white\">Đăng ký</a>\r\n        </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nimport Cookies from 'js-cookie';\r\nimport axios from 'axios';\r\nexport default {\r\n  \r\n  name: 'HeaderScreen',\r\n  data() {\r\n    return {\r\n      isAuthenticated: false,\r\n      showDropdown: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.checkAuthentication();\r\n  },\r\n  methods: {\r\n    checkAuthentication() {\r\n      const token = Cookies.get(\"auth\"); // Thay \"auth\" bằng tên của cookie lưu token\r\n\r\n      if (token) {\r\n        // Nếu có token, tức là đã đăng nhập\r\n        this.isAuthenticated = true;\r\n      } else {\r\n        // Nếu không có token, tức là chưa đăng nhập\r\n        this.isAuthenticated = false;\r\n      }\r\n    },\r\n\r\n    toggleDropdown() {\r\n      this.showDropdown = !this.showDropdown;\r\n    },\r\n\r\n    async logout(){\r\n      try{\r\n        const token = Cookies.get(\"auth\");\r\n        const formData = new FormData();\r\n        formData.append('token', token);\r\n\r\n        const response = await axios.post('http://localhost:8086/auth/logout', formData, {\r\n        headers: {\r\n          'Content-Type': 'multipart/form-data',\r\n          },\r\n        });\r\n        console.log(response);\r\n        this.deleteCookie(\"auth\");\r\n        localStorage.removeItem(\"idUser\");\r\n\r\n        this.$router.push('/login'); // Chuyển hướng đến trang home\r\n\r\n      }catch(error){\r\n        console.error('Get infor failed', error.response.data);\r\n      }\r\n    },\r\n\r\n    deleteCookie(name) {\r\n    document.cookie = `${name}=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;`;\r\n    }\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n"],"mappings":";AAiDA,OAAOA,OAAM,MAAO,WAAW;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EAEbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,KAAK;MACtBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACPD,mBAAmBA,CAAA,EAAG;MACpB,MAAME,KAAI,GAAIT,OAAO,CAACU,GAAG,CAAC,MAAM,CAAC,EAAE;;MAEnC,IAAID,KAAK,EAAE;QACT;QACA,IAAI,CAACL,eAAc,GAAI,IAAI;MAC7B,OAAO;QACL;QACA,IAAI,CAACA,eAAc,GAAI,KAAK;MAC9B;IACF,CAAC;IAEDO,cAAcA,CAAA,EAAG;MACf,IAAI,CAACN,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC,CAAC;IAED,MAAMO,MAAMA,CAAA,EAAE;MACZ,IAAG;QACD,MAAMH,KAAI,GAAIT,OAAO,CAACU,GAAG,CAAC,MAAM,CAAC;QACjC,MAAMG,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;QAC/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAEN,KAAK,CAAC;QAE/B,MAAMO,QAAO,GAAI,MAAMf,KAAK,CAACgB,IAAI,CAAC,mCAAmC,EAAEJ,QAAQ,EAAE;UACjFK,OAAO,EAAE;YACP,cAAc,EAAE;UAChB;QACF,CAAC,CAAC;QACFC,OAAO,CAACC,GAAG,CAACJ,QAAQ,CAAC;QACrB,IAAI,CAACK,YAAY,CAAC,MAAM,CAAC;QACzBC,YAAY,CAACC,UAAU,CAAC,QAAQ,CAAC;QAEjC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;MAE/B,CAAC,QAAMC,KAAK,EAAC;QACXP,OAAO,CAACO,KAAK,CAAC,kBAAkB,EAAEA,KAAK,CAACV,QAAQ,CAACb,IAAI,CAAC;MACxD;IACF,CAAC;IAEDkB,YAAYA,CAACnB,IAAI,EAAE;MACnByB,QAAQ,CAACC,MAAK,GAAK,GAAE1B,IAAK,mDAAkD;IAC5E;EACF;AAGF"},"metadata":{},"sourceType":"module","externalDependencies":[]}