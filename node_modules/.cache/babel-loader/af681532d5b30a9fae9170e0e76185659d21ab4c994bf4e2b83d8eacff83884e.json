{"ast":null,"code":"import HomeScreen from \"./components/client/HomeScreen.vue\";\nimport HeaderScreen from \"./components/client/HeaderScreen.vue\";\nimport FooterScreen from \"./components/client/FooterScreen.vue\";\nimport TrainingScreen from \"./components/client/TrainingScreen.vue\";\nimport SupportScreen from \"./components/client/SupportScreen.vue\";\nimport CourseScreen from \"./components/client/CourseScreen.vue\";\nimport LoginScreen from \"./components/client/LoginScreen.vue\";\nimport RegisterScreen from \"./components/client/RegisterScreen.vue\";\nimport { ref } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  created() {},\n  setup(props, {\n    emit\n  }) {\n    const courses = ref([]);\n    const page = ref(0);\n    const records = 6;\n    const totalPage = ref(0);\n    const search = ref('');\n    const priceFrom = ref(0);\n    const priceTo = ref(0);\n    const onPriceFromChanged = value => {\n      priceFrom.value = value;\n    };\n    const onPriceToChanged = value => {\n      priceTo.value = value;\n    };\n    const onSearchChange = keyword => {\n      search.value = keyword;\n      getAllCourses();\n    };\n    const getAllCourses = async () => {\n      try {\n        const res = await axios.get(\"http://localhost:8080/api/course\", {\n          params: {\n            page: page.value,\n            records: records,\n            search: search.value,\n            priceFrom: priceFrom.value,\n            priceTo: priceTo.value\n          }\n        });\n        courses.value = res.data.data;\n        totalPage.value = res.data.totalPage;\n\n        // Phát ra sự kiện để thông báo danh sách khóa học mới cho component con\n        emit(\"coursesChanged\", courses.value);\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    const onPageChanged = newPage => {\n      page.value = newPage;\n      getAllCourses();\n    };\n    getAllCourses();\n    return {\n      totalPage,\n      courses,\n      onPageChanged,\n      onSearchChange,\n      onPriceFromChanged,\n      onPriceToChanged\n    };\n  },\n  name: 'App',\n  components: {\n    HomeScreen,\n    TrainingScreen,\n    CourseScreen,\n    SupportScreen,\n    HeaderScreen,\n    FooterScreen,\n    LoginScreen,\n    RegisterScreen\n  },\n  data() {\n    return {\n      page: \"course\"\n    };\n  }\n};","map":{"version":3,"names":["HomeScreen","HeaderScreen","FooterScreen","TrainingScreen","SupportScreen","CourseScreen","LoginScreen","RegisterScreen","ref","axios","created","setup","props","emit","courses","page","records","totalPage","search","priceFrom","priceTo","onPriceFromChanged","value","onPriceToChanged","onSearchChange","keyword","getAllCourses","res","get","params","data","error","console","log","onPageChanged","newPage","name","components"],"sources":["F:\\VueJS\\thaylam\\Elearning\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <header-screen v-if=\"page!='login' && page!='register'\"></header-screen>\n    <home-screen v-if=\"page === 'home'\" :courses=\"courses\"></home-screen>\n    <training-screen v-if=\"page === 'training'\"></training-screen>\n    <support-screen v-if=\"page === 'support'\"></support-screen>\n    <course-screen v-if=\"page === 'course'\" :courses=\"courses\" :totalPage=\"totalPage\" @search=\"onSearchChange\" @priceFrom=\"onPriceFromChanged\" @priceTo=\"onPriceToChanged\"  @pageChanged=\"onPageChanged\"></course-screen>\n    <login-screen v-if=\"page === 'login'\"></login-screen>\n    <register-screen v-if=\"page === 'register'\"></register-screen>\n    <footer-screen v-if=\"page!='login' && page!='register'\"></footer-screen>\n  </div>\n</template>\n\n<script>\n  import HomeScreen from \"./components/client/HomeScreen.vue\";\n  import HeaderScreen from \"./components/client/HeaderScreen.vue\";\n  import FooterScreen from \"./components/client/FooterScreen.vue\";\n  import TrainingScreen from \"./components/client/TrainingScreen.vue\";\n  import SupportScreen from \"./components/client/SupportScreen.vue\";\n  import CourseScreen from \"./components/client/CourseScreen.vue\";\n  import LoginScreen from \"./components/client/LoginScreen.vue\";\n  import RegisterScreen from \"./components/client/RegisterScreen.vue\";\n  import { ref } from \"vue\";\n  import axios from \"axios\";\n\n\nexport default {\n  created () {\n  },\n  setup(props, { emit }){\n    const courses = ref([]);\n\n    const page = ref(0);\n    const records = 6;\n    const totalPage = ref(0);\n    const search = ref('');\n    const priceFrom = ref(0);\n    const priceTo = ref(0);\n\n    const onPriceFromChanged = (value) => {\n      priceFrom.value = value;\n    }\n\n    const onPriceToChanged = (value) => {\n      priceTo.value = value;\n    }\n\n    const onSearchChange = (keyword) => {\n      search.value = keyword;\n\n      getAllCourses();\n    }\n\n    const getAllCourses = async () => {\n      try\n      {\n        const res = await axios.get(\"http://localhost:8080/api/course\", {\n          params: {\n            page: page.value,\n            records: records,\n            search: search.value,\n            priceFrom: priceFrom.value,\n            priceTo: priceTo.value\n          }\n        });\n\n        courses.value = res.data.data;\n        totalPage.value = res.data.totalPage;\n        \n        // Phát ra sự kiện để thông báo danh sách khóa học mới cho component con\n        emit(\"coursesChanged\", courses.value);\n\n      } catch (error) {\n        console.log(error)\n      }\n    }\n\n    const onPageChanged  = (newPage) => {\n      page.value = newPage;\n      getAllCourses();\n    }\n\n    getAllCourses();\n\n    return {\n      totalPage,\n      courses,\n      onPageChanged,\n      onSearchChange,\n      onPriceFromChanged,\n      onPriceToChanged\n    }\n  },\n  name: 'App',\n  components: {\n    HomeScreen,\n    TrainingScreen,\n    CourseScreen,\n    SupportScreen,\n    HeaderScreen,\n    FooterScreen,\n    LoginScreen,\n    RegisterScreen\n  },\n  data(){\n    return{\n      page: \"course\",\n    };\n  }\n}\n</script>\n"],"mappings":"AAcE,OAAOA,UAAS,MAAO,oCAAoC;AAC3D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,OAAOC,aAAY,MAAO,uCAAuC;AACjE,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,WAAU,MAAO,qCAAqC;AAC7D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,SAASC,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAG3B,eAAe;EACbC,OAAMA,CAAA,EAAK,CACX,CAAC;EACDC,KAAKA,CAACC,KAAK,EAAE;IAAEC;EAAK,CAAC,EAAC;IACpB,MAAMC,OAAM,GAAIN,GAAG,CAAC,EAAE,CAAC;IAEvB,MAAMO,IAAG,GAAIP,GAAG,CAAC,CAAC,CAAC;IACnB,MAAMQ,OAAM,GAAI,CAAC;IACjB,MAAMC,SAAQ,GAAIT,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMU,MAAK,GAAIV,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMW,SAAQ,GAAIX,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMY,OAAM,GAAIZ,GAAG,CAAC,CAAC,CAAC;IAEtB,MAAMa,kBAAiB,GAAKC,KAAK,IAAK;MACpCH,SAAS,CAACG,KAAI,GAAIA,KAAK;IACzB;IAEA,MAAMC,gBAAe,GAAKD,KAAK,IAAK;MAClCF,OAAO,CAACE,KAAI,GAAIA,KAAK;IACvB;IAEA,MAAME,cAAa,GAAKC,OAAO,IAAK;MAClCP,MAAM,CAACI,KAAI,GAAIG,OAAO;MAEtBC,aAAa,CAAC,CAAC;IACjB;IAEA,MAAMA,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IACA;QACE,MAAMC,GAAE,GAAI,MAAMlB,KAAK,CAACmB,GAAG,CAAC,kCAAkC,EAAE;UAC9DC,MAAM,EAAE;YACNd,IAAI,EAAEA,IAAI,CAACO,KAAK;YAChBN,OAAO,EAAEA,OAAO;YAChBE,MAAM,EAAEA,MAAM,CAACI,KAAK;YACpBH,SAAS,EAAEA,SAAS,CAACG,KAAK;YAC1BF,OAAO,EAAEA,OAAO,CAACE;UACnB;QACF,CAAC,CAAC;QAEFR,OAAO,CAACQ,KAAI,GAAIK,GAAG,CAACG,IAAI,CAACA,IAAI;QAC7Bb,SAAS,CAACK,KAAI,GAAIK,GAAG,CAACG,IAAI,CAACb,SAAS;;QAEpC;QACAJ,IAAI,CAAC,gBAAgB,EAAEC,OAAO,CAACQ,KAAK,CAAC;MAEvC,EAAE,OAAOS,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB;IACF;IAEA,MAAMG,aAAY,GAAMC,OAAO,IAAK;MAClCpB,IAAI,CAACO,KAAI,GAAIa,OAAO;MACpBT,aAAa,CAAC,CAAC;IACjB;IAEAA,aAAa,CAAC,CAAC;IAEf,OAAO;MACLT,SAAS;MACTH,OAAO;MACPoB,aAAa;MACbV,cAAc;MACdH,kBAAkB;MAClBE;IACF;EACF,CAAC;EACDa,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVrC,UAAU;IACVG,cAAc;IACdE,YAAY;IACZD,aAAa;IACbH,YAAY;IACZC,YAAY;IACZI,WAAW;IACXC;EACF,CAAC;EACDuB,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJf,IAAI,EAAE;IACR,CAAC;EACH;AACF"},"metadata":{},"sourceType":"module","externalDependencies