{"ast":null,"code":"import SidebarteacherScreen from './SidebarteacherScreen.vue';\nimport axios from \"axios\";\nimport { useRoute } from 'vue-router';\nexport default {\n  components: {\n    SidebarteacherScreen\n  },\n  data() {\n    return {\n      selectedFile: null,\n      // Tên mặc định khi chưa có tệp nào được chọn\n      lesson: {\n        name: \"\",\n        thematicID: 0,\n        urlVideo: \"\",\n        duration: \"\"\n      }\n    };\n  },\n  methods: {\n    async uploadLesson() {\n      try {\n        if (this.selectedFile != null) {\n          //create form data send with api\n          const formData = new FormData();\n          formData.append('file', this.selectedFile);\n\n          //get thematic id from url\n          const route = useRoute();\n          const courseId = route.params.id;\n          await axios.post(\"http://localhost:8087/api/upload\", formData, {\n            headers: {\n              'Content-Type': 'multipart/form-data' // Important for handling files\n            }\n          }).then(response => {\n            if (response.status == 200) {\n              const res = response.data.data;\n              this.lesson.duration = res.duration;\n              this.lesson.urlVideo = res.url;\n              this.lesson.thematicID = courseId;\n              console.log(this.lesson);\n            }\n          });\n        }\n      } catch (error) {\n        console.error('Error uploading file:', error);\n      }\n    },\n    handleFileChange(event) {\n      const inputFile = event.target;\n      if (inputFile.files.length > 0) {\n        // Lấy tên của tệp đầu tiên nếu có\n        this.selectedFile = event.target.files[0];\n      } else {\n        this.selectedFile = \"Chưa chọn tệp\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["SidebarteacherScreen","axios","useRoute","components","data","selectedFile","lesson","name","thematicID","urlVideo","duration","methods","uploadLesson","formData","FormData","append","route","course