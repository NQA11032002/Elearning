icon: \"fa-solid fa-circle-exclamation\"\n            },\n        };\n    },\n    methods: {\n        async changePass(){\n            try{\n                const queryParams = Cookies.get(\"userID\");\n                const token = Cookies.get(\"auth\");\n                const body = {\n                    password: this.user.password,\n                };\n                const options = {\n                    headers: { 'Authorization': `Bearer ${token}` },\n                    params: {\n                        currentPassword: this.user.currentPassword\n                    },\n                };\n                if(this.user.password !== this.user.repassword){\n                    this.isValidate(false, \"Mật khẩu nhập lại không chính xác\");\n                    return;\n                }else{\n                    const apiObject = findApiByName(\"user\", \"updateUser\").url+\"/\"+queryParams;\n                    const response = await axios.put(apiObject, body, options);\n                    console.log(response.data.status);\n\n                    if(this.user.password.length < 6){\n                    this.isValidate(false, \"Mật khẩu mới phải trên 6 kí tự\");\n                    return;\n                    }\n                    \n                    if(response.data.status === \"BAD_REQUEST\"){\n                    this.isValidate(false, \"Mật khẩu hiện tại không đúng\");\n                    return;\n                    }\n                    this.user.isChangeInfor = true;\n                    this.contents.title = \"Đổi mật khẩu thành công\";\n                    this.contents.status = true;\n                    this.contents.color = \"green-500\";\n                    this.contents.icon = \"fa-regular fa-circle-check\";\n                }\n\n            }catch(error){\n                console.error('Changepass fail', error);\n            }\n    },\n        isValidate(error, message){\n            this.validate.error = error;\n            this.validate.message = message;\n        }\n    },\n};\n</script>"],"mappings":"AAgCA,OAAOA,mBAAkB,MAAO,mCAAmC;AACnE,OAAOC,OAAM,MAAO,WAAW;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,aAAY,QAAS,+BAA+B;AAC7D,OAAOC,YAAW,MAAO,4BAA4B;AACrD,eAAe;EACXC,UAAU,EAAE;IACRL,mBAAmB;IACnBI;EACJ,CAAC;EACDE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFC,eAAe,EAAE,EAAE;QACnBC,QAAQ,EAAE,EAAE;QACZC,UAAU,EAAE,EAAE;QACdC,aAAa,EAAE;MACnB,CAAC;MACDC,QAAQ,EAAE;QACNC,KAAK,EAAE,KAAK;QACZC,OAAO,EAAE;MACb,CAAC;MACDC,QAAQ,EAAE;QACNC,KAAK,EAAE,uBAAuB;QAC9BC,MAAM,EAAE,KAAK;QACbC,KAAK,EAAE,SAAS;QAChBC,IAAI,EAAE;MACV;IACJ,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAE;MACd,IAAG;QACC,MAAMC,WAAU,GAAIrB,OAAO,CAACsB,GAAG,CAAC,QAAQ,CAAC;QACzC,MAAMC,KAAI,GAAIvB,OAAO,CAACsB,GAAG,CAAC,MAAM,CAAC;QACjC,MAAME,IAAG,GAAI;UACThB,QAAQ,EAAE,IAAI,CAACF,IAAI,CAACE;QACxB,CAAC;QACD,MAAMiB,OAAM,GAAI;UACZC,OAAO,EAAE;YAAE,eAAe,EAAG,UAASH,KAAM;UAAE,CAAC;UAC/CI,MAAM,EAAE;YACJpB,eAAe,EAAE,IAAI,CAACD,IAAI,CAACC;UAC/B;QACJ,CAAC;QACD,IAAG,IAAI,CAACD,IAAI,CAACE,QAAO,KAAM,IAAI,CAACF,IAAI,CAACG,UAAU,EAAC;UAC3C,IAAI,CAACmB,UAAU,CAAC,KAAK,EAAE,mCAAmC,CAAC;UAC3D;QACJ,CAAC,MAAI;UACD,MAAMC,SAAQ,GAAI3B,aAAa,CAAC,MAAM,EAAE,YAAY,CAAC,CAAC4B,GAAG,GAAC,GAAG,GAACT,WAAW;UACzE,MAAMU,QAAO,GAAI,MAAM9B,KAAK,CAAC+B,GAAG,CAACH,SAAS,EAAEL,IAAI,EAAEC,OAAO,CAAC;UAC1DQ,OAAO,CAACC,GAAG,CAACH,QAAQ,CAAC1B,IAAI,CAACW,MAAM,CAAC;UAEjC,IAAG,IAAI,CAACV,IAAI,CAACE,QAAQ,CAAC2B,MAAK,GAAI,CAAC,EAAC;YACjC,IAAI,CAACP,UAAU,CAAC,KAAK,EAAE,gCAAgC,CAAC;YACxD;UACA;UAEA,IAAGG,QAAQ,CAAC1B,IAAI,CAACW,MAAK,KAAM,aAAa,EAAC;YAC1C,IAAI,CAACY,UAAU,CAAC,KAAK,EAAE,8BAA8B,CAAC;YACtD;UACA;UACA,IAAI,CAACtB,IAAI,CAACI,aAAY,GAAI,IAAI;UAC9B,IAAI,CAACI,QAAQ,CAACC,KAAI,GAAI,yBAAyB;UAC/C,IAAI,CAACD,QAAQ,CAACE,MAAK,GAAI,IAAI;UAC3B,IAAI,CAACF,QAAQ,CAACG,KAAI,GAAI,WAAW;UACjC,IAAI,CAACH,QAAQ,CAACI,IAAG,GAAI,4BAA4B;QACrD;MAEJ,CAAC,QAAMN,KAAK,EAAC;QACTqB,OAAO,CAACrB,KAAK,CAAC,iBAAiB,EAAEA,KAAK,CAAC;MAC3C;IACR,CAAC;IACGgB,UAAUA,CAAChB,KAAK,EAAEC,OAAO,EAAC;MACtB,IAAI,CAACF,QAAQ,CAACC,KAAI,GAAIA,KAAK;MA