h (error) {\n                // Xử lý lỗi từ API (error.response.data) nếu cần\n                console.error('Login failed', error.response.data);\n            }\n        },\n    },\n};\n</script>\n"],"mappings":"AAkCA,OAAO,gCAAgC;AACvC,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE;IAChB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,KAAKA,CAAA,EAAG;MACV,IAAI;QACA;QACA,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,kCAAkC,EAAE;UAClEP,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;;QAEF;QACAO,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAEH,QAAQ,CAACP,IAAI,CAACG,KAAK,CAAC;;QAGpD;QACAQ,KAAK,CAAC,kBAAkB,CAAC;QACzB;MACJ,EAAE,OAAOC,KAAK,EAAE;QACZ;QACAH,OAAO,CAACG,KAAK,CAAC,cAAc,EAAEA,KAAK,CAACL,QAAQ,CAACP,IAAI,CAAC;MACtD;IACJ;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}