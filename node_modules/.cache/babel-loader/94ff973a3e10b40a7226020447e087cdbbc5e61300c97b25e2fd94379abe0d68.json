,\n\n\n    async logout() {\n      try { \n        const apiObject = findApiByName(\"auth\", \"logout\").url;\n        const token = Cookies.get(\"auth\");\n        const formData = new FormData();\n        formData.append('token', token);\n        await axios.post(apiObject, formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data',\n          },\n        }).then((res) => {\n          if(res.status === 200) {\n            this.deleteCookie(\"auth\");\n            this.deleteCookie(\"userID\");\n            this.deleteCookie(\"role\");\n            this.$router.push('/login');\n          }\n        })\n        \n      } catch (error) {\n        console.error('Logout fail', error);\n      }\n    },\n\n    deleteCookie(name) {\n      document.cookie = `${name}=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;`;\n    }\n  },\n\n\n}\n</script>\n"],"mappings":";AAgEA,OAAOA,OAAM,MAAO,WAAW;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,aAAY,QAAS,+BAA+B;AAC7D;;AAEA,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,KAAK;MACtBC,YAAY,EAAE,KAAK;MACnBC,IAAI,EAAEP,OAAO,CAACQ,GAAG,CAAC,MAAM;IAC1B,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACPD,mBAAmBA,CAAA,EAAG;MACpB,MAAME,KAAI,GAAIZ,OAAO,CAACQ,GAAG,CAAC,MAAM,CAAC,EAAE;;MAEnC,IAAII,KAAK,EAAE;QACT,IAAI,CAACP,eAAc,GAAI,IAAI;MAC7B,OAAO;QACL,IAAI,CAACA,eAAc,GAAI,KAAK;MAC9B;IACF,CAAC;IAEDQ,cAAcA,CAAA,EAAG;MACf,IAAI,CAACP,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC,CAAC;IAGD,MAAMQ,MAAMA,CAAA,EAAG;MACb,IAAI;QACF,MAAMC,SAAQ,GAAIb,aAAa,CAAC,MAAM,EAAE,QAAQ,CAAC,CAACc,GAAG;QACrD,MAAMJ,KAAI,GAAIZ,OAAO,CAACQ,GAAG,CAAC,MAAM,CAAC;QACjC,MAAMS,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;QAC/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAEP,KAAK,CAAC;QAC/B,MAAMX,KAAK,CAACmB,IAAI,CAACL,SAAS,EAAEE,QAAQ,EAAE;UACpCI,OAAO,EAAE;YACP,cAAc,EAAE;UAClB;QACF,CAAC,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;UACf,IAAGA,GAAG,CAACC,MAAK,KAAM,GAAG,EAAE;YACrB,IAAI,CAACC,YAAY,CAAC,MAAM,CAAC;YACzB,IAAI,CAACA,YAAY,CAA