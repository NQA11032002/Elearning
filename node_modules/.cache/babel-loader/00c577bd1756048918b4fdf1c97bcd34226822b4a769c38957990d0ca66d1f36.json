{"ast":null,"code":"import HeaderScreen from \"./components/client/common/HeaderScreen.vue\";\nimport FooterScreen from \"./components/client/common/FooterScreen.vue\";\nimport { findApiByName } from './assets/js/apiUtil.js';\nimport axios from 'axios';\nexport default {\n  setup() {},\n  name: \"App\",\n  components: {\n    HeaderScreen,\n    FooterScreen\n  },\n  data() {\n    return {\n      page: \"home\",\n      user: {\n        fullName: ''\n      }\n    };\n  },\n  async loadInfor() {\n    try {\n      const queryParams = localStorage.getItem(\"idUser\");\n      if (!queryParams) {\n        console.error('idUser is null or undefined');\n        return;\n      }\n      const apiObject = findApiByName(\"customer\", \"findByID\").url + \"/\" + queryParams;\n      const token = localStorage.getItem(\"auth\");\n      const response = await axios.get(apiObject, {\n        headers: {\n          'Authorization': `Bearer ${token}`\n        }\n      });\n      this.user.fullName = response.data.data.fullName;\n      console.log(response.data.data);\n    } catch (error) {\n      console.error('Get infor failed', error.response.data);\n    }\n  }\n};","map":{"version":3,"names":["HeaderScreen","FooterScreen","findApiByName","axios","setup","name","components","data","page","user","fullName","loadInfor","queryParams","localStorage","getItem","console","error","apiObject","url","token","response","get","headers","log"],"sources":["F:\\VueJS\\Elearning-Frontend\\Elearning\\src\\App.vue"],"sourcesContent":["<template>\n  <HeaderScreen></HeaderScreen>\n\n  <div class=\"py-8 w-full max-sm:py-0 bg-gray-50\">\n    <router-view :user=\"user\"></router-view>\n  </div>\n\n  <footer-screen></footer-screen>\n</template>\n\n<script>\nimport HeaderScreen from \"./components/client/common/HeaderScreen.vue\";\nimport FooterScreen from \"./components/client/common/FooterScreen.vue\";\nimport { findApiByName } from './assets/js/apiUtil.js';\nimport axios from 'axios';\nexport default {\n  setup() { },\n  name: \"App\",\n  components: {\n    HeaderScreen,\n    FooterScreen,\n  },\n  data() {\n    return {\n      page: \"home\",\n      user: {\n        fullName: '',\n      }\n    }\n  },\n  async loadInfor() {\n    try {\n      const queryParams = localStorage.getItem(\"idUser\");\n      if (!queryParams) {\n        console.error('idUser is null or undefined');\n        return;\n      }\n      const apiObject = findApiByName(\"customer\", \"findByID\").url + \"/\" + queryParams;\n      const token = localStorage.getItem(\"auth\");\n      const response = await axios.get(apiObject, {\n        headers: {\n          'Authorization': `Bearer ${token}`\n        }\n      });\n\n      this.user.fullName = response.data.data.fullName;\n\n      console.log(response.data.data);\n    } catch (error) {\n      console.error('Get infor failed', error.response.data);\n    }\n  }\n}\n</script>\n"],"mappings":"AAWA,OAAOA,YAAW,MAAO,6CAA6C;AACtE,OAAOC,YAAW,MAAO,6CAA6C;AACtE,SAASC,aAAY,QAAS,wBAAwB;AACtD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,KAAKA,CAAA,EAAG,CAAE,CAAC;EACXC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVN,YAAY;IACZC;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,MAAM;MACZC,IAAI,EAAE;QACJC,QAAQ,EAAE;MACZ;IACF;EACF,CAAC;EACD,MAAMC,SAASA,CAAA,EAAG;IAChB,IAAI;MACF,MAAMC,WAAU,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;MAClD,IAAI,CAACF,WAAW,EAAE;QAChBG,OAAO,CAACC,KAAK,CAAC,6BAA6B,CAAC;QAC5C;MACF;MACA,MAAMC,SAAQ,GAAIf,aAAa,CAAC,UAAU,EAAE,UAAU,CAAC,CAACgB,GAAE,GAAI,GAAE,GAAIN,WAAW;MAC/E,MAAMO,KAAI,GAAIN,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC;MAC1C,MAAMM,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAACJ,SAAS,EAAE;QAC1CK,OAAO,EAAE;UACP,eAAe,EAAG,UAASH,KAAM;QACnC;MACF,CAAC,CAAC;MAEF,IAAI,CAACV,IAAI,CAACC,QAAO,GAAIU,QAAQ,CAACb,IAAI,CAACA,IAAI,CAACG,QAAQ;MAEhDK,OAAO,CAACQ,GAAG,CAACH,QAAQ,CAACb,IAAI,CAACA,IAAI,CAAC;IACjC,EAAE,OAAOS,KAAK,EAAE;MACdD,OAAO,CAACC,KAAK,CAAC,kBAAkB,EAAEA,KAAK,CAACI,QAAQ,CAACb,IAAI,CAAC;IACxD;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}