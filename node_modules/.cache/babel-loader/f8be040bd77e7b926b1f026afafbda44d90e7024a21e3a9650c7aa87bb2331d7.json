rounded-lg border py-2 w-32 text-sm text-gray-400\">Thêm khóa học</button>\n        </a>\n      </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { findApiByName } from \"../../../assets/js/apiUtil.js\";\nimport { auth } from \"../../../assets/js/auth.js\";\nimport Cookies from 'js-cookie';\n\nexport default {\n  mounted() {\n    const getCourse = async () => {\n      try {\n\n        //get url API\n        const apiObject = findApiByName(\"order\", \"findCourse\").url;\n\n        //get all course ordered\n        const res = await axios.get(apiObject + auth());\n\n        if (res.status == 200) {\n          if (res.data.data.length > 0) {\n            //get courseID from order\n            res.data.data.forEach(element => {\n\n              //get course by courseID from order \n              this.getCourseByID(element.courseID);\n            });\n          }\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    }\n    getCourse();\n  },\n  setup() {\n\n  },\n  methods: {\n    //get course by id from arrayID\n    async getCourseByID(courseID) {\n      //get url API\n      const apiObject = findApiByName(\"course\", \"findCourseByID\").url;\n\n      const res = await axios.get(apiObject + courseID);\n\n      if (res.status == 200) {\n        this.courses.push(res.data.data)\n      }\n    },\n\n    async getUserByID(userID) {\n      //get url API\n      const apiObject = findApiByName(\"customer\", \"findUser\").url;\n\n      if (Cookies.get('auth') != undefined) {\n        const formData = new FormData();\n        formData.append('token', Cookies.get('auth'));\n\n        const res = await axios.get(apiObject + userID, formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data',\n          },\n        });\n        console.log(res)\n\n        if (res.status == 200) {\n          console.log(res)\n        }\n      }\n    }\n  },\n  components: {\n\n  },\n  data() {\n    return {\n      arrayID: [],\n      courses: []\n    };\n  }\n}\n\n</script>\n"],"mappings":";AAsCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,aAAY,QAAS,+BAA+B;AAC7D,SAASC,IAAG,QAAS,4BAA4B;AACjD,OAAOC,OAAM,MAAO,WAAW;AAE/B,eAAe;EACbC,OAAOA,CAAA,EAAG;IACR,MAAMC,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAC5B,IAAI;QAEF;QACA,MAAMC,SAAQ,GAAIL,aAAa,CAAC,OAAO,EAAE,YAAY,CAAC,CAACM,GAAG;;QAE1D;QACA,MAAMC,GAAE,GAAI,MAAMR,KAAK,CAACS,GAAG,CAACH,SAAQ,GAAIJ,IAAI,CAAC,CAAC,CAAC;QAE/C,IAAIM,GAAG,CAACE,MAAK,IAAK,GAAG,EAAE;UACrB,IAAIF,GAAG,CAACG,IAAI,CAACA,IAAI,CAACC,MAAK,GAAI,CAAC,EAAE;YAC5B;YACAJ,GAAG,CAACG,IAAI,CAACA,IAAI,CAACE,OAAO,CAACC,OAAM,IAAK;cAE/B;cACA,IAAI,CAACC,aAAa,CAACD,OAAO,CAACE,QAAQ,CAAC;YACtC,CAAC,CAAC;UACJ;QACF;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF;IACAZ,SAAS,CAAC,CAAC;EACb,CAAC;EACDe,KAAKA,CAAA,EAAG,CAER,CAAC;EACDC,OAAO,EAAE;IACP;IACA,MAAMN,aAAaA,CAACC,QAAQ,EAAE;MAC5B;MACA,MAAMV,SAAQ,GAAIL,aAAa,CAAC,QAAQ,EAAE,gBAAgB,CAAC,CAACM,GAAG;MAE/D,MAAMC,GAAE,GAAI,MAAMR,KAAK,CAACS,GAAG,CAACH,SAAQ,GAAIU,QAAQ,CAAC;MAEjD,IAAIR,GAAG,CAACE,MAAK,IAAK,GAAG,EAAE;QACrB,IAAI,CAACY,OAAO,CAACC,IAAI,CAACf,GAAG,CAACG,IAAI,CAACA,IAAI;MACjC;IACF,CAAC;IAED,MAAMa,WAAWA,CAACC,MAAM,EAAE;MACxB;MACA,MAAMnB,SAAQ,GAAIL,aAAa,CAAC,UAAU,EAAE,UAAU,CAAC,CAACM,GAAG;MAE3D,IAAIJ,OAAO,CAACM,GAAG,CAAC,MAAM,KAAKiB,SAAS,EAAE;QACpC,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;QAC/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE1B,OAAO,CAACM,GAAG,CAAC,MAAM,CAAC,CAAC;QAE7C,MAAMD,GAAE,GAAI,MAAMR,KAAK,CAACS,GAAG,CAACH,SAAQ,GAAImB,MAAM,EAAEE,QAAQ,EAAE;UACxDG,OAAO,EAAE;YACP,cAAc,EAAE;UAClB;QACF,CAAC,CAAC;QACFZ,OAAO,CAACC,GAAG,CAACX,GAAG;QAEf,IAAIA,GAAG,CAACE,MAAK,IAAK,GAAG,EAAE;UACrBQ,OAAO,CAACC,GAAG,CAACX,GAAG;QACjB;MACF;IACF;EACF,CAAC;EACDuB,UAAU,EAAE,CAEZ,CAAC;EACDpB,IAAIA,CAAA,EAAG;IACL,OAAO;MACLqB,OAAO,EAAE,EAAE;MACXV,OAAO,EAAE;IACX,CAAC;EACH;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}