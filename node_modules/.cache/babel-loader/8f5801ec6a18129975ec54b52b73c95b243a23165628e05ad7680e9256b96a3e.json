records,\n      \"filters\" : []\n    })\n\n    console.log(filter.value)\n\n    const getAllCourses = async () => {\n      try\n      {\n        const res = await axios.get(\"http://localhost:8080/api/course\", filter.value, {\n          headers: {\n              'Content-Type': 'application/json'\n          }\n        });\n\n        courses.value = res.data.data;\n        totalPage.value = res.data.totalPage;\n        \n        // Phát ra sự kiện để thông báo danh sách khóa học mới cho component con\n        emit(\"coursesChanged\", courses.value);\n\n      } catch (error) {\n        console.log(error)\n      }\n    }\n\n    const onPageChanged  = (newPage) => {\n      page.value = newPage;\n      getAllCourses();\n    }\n\n    getAllCourses();\n\n    return {\n      totalPage,\n      courses,\n      onPageChanged \n    }\n  },\n  name: 'App',\n  components: {\n    HomeScreen,\n    TrainingScreen,\n    CourseScreen,\n    SupportScreen,\n    HeaderScreen,\n    FooterScreen,\n    LoginScreen,\n    RegisterScreen\n  },\n  data(){\n    return{\n      page: \"course\",\n    };\n  }\n}\n</script>\n"],"mappings":"AAcE,OAAOA,UAAS,MAAO,oCAAoC;AAC3D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,OAAOC,aAAY,MAAO,uCAAuC;AACjE,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,WAAU,MAAO,qCAAqC;AAC7D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,SAASC,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAG3B,eAAe;EACbC,OAAMA,CAAA,EAAK,CACX,CAAC;EACDC,KAAKA,CAACC,KAAK,EAAE;IAAEC;EAAK,CAAC,EAAC;IACpB,MAAMC,OAAM,GAAIN,GAAG,CAAC,EAAE,CAAC;IAEvB,MAAMO,IAAG,GAAIP,GAAG,CAAC,CAAC,CAAC;IACnB,MAAMQ,OAAM,GAAI,CAAC;IACjB,MAAMC,SAAQ,GAAIT,GAAG,CAAC,CAAC,CAAC;IAExB,MAAMU,MAAK,GAAIV,GAAG,CAAC;MACjB,MAAK,EAAIO,IAAI,CAACI,KAAK;MACnB,SAAQ,EAAIH,OAAO;MACnB,SAAQ,EAAI;IACd,CAAC;IAEDI,OAAO,CAACC,GAAG,CAACH,MAAM,CAACC,KAAK;IAExB,MAAMG,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IACA;QACE,MAAMC,GAAE,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,kCAAkC,EAAEN,MAAM,CAACC,KAAK,EAAE;UAC5EM,OAAO,EAAE;YACL,cAAc,EAAE;UACpB;QACF,CAAC,CAAC