{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      user: {\n        userName: '',\n        fullName: '',\n        phone: '',\n        password: '',\n        repeatPassword: '',\n        checkPasswordLength: true\n      }\n    };\n  },\n  methods: {\n    async register() {\n      // Kiểm tra xem mật khẩu có trùng khớp không\n      if (this.user.password !== this.user.repeatPassword) {\n        alert('Mật khẩu nhập lại không chính xác');\n        return;\n      }\n      if (this.user.password.length < 6) {\n        this.user.checkPasswordLength = false;\n        return;\n      }\n      try {\n        // Gửi yêu cầu POST đến API Spring Boot để đăng ký\n        const response = await axios.post('http://localhost:8086/auth/register', {\n          userName: this.user.userName,\n          password: this.user.password,\n          fullName: this.user.fullName,\n          phone: this.user.phone\n        });\n\n        // Xử lý dữ liệu nhận được từ API (response.data) nếu cần\n        console.log('Đăng ký tài khoản thành công', response.data);\n\n        // Hiển thị thông báo và chuyển hướng sau khi đăng ký thành công\n        alert('Đăng ký tài khoản thành công');\n        this.$router.push('/login'); // Chuyển hướng đến trang login\n      } catch (error) {\n        // Xử lý lỗi từ API (error.response.data) nếu cần\n        console.error('Registration failed', error.response.data);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","user","userName","fullName","phone","password","repeatPassword","checkPasswordLength","methods","register","alert","length","response","post","console","log","$router","push","error"],"sources":["F:\\VueJS\\Elearning-Frontend\\Elearning\\src\\components\\client\\RegisterScreen.vue"],"sourcesContent":["<template>\n    <body class=\"LoginForm\">\n        <div class=\"LoginForm_container\" id=\"container\">\n            <div class=\"form-container log-in-container\">\n                <form @submit.prevent=\"register\">\n                    <h1>Create Account</h1>\n                    <span>Create your new account</span>\n                    <input v-model=\"user.userName\" type=\"text\" placeholder=\"Username\" />\n                    <input v-model=\"user.fullName\" type=\"text\" placeholder=\"Full name\" />\n                    <input v-model=\"user.phone\" type=\"text\" placeholder=\"Phone number\" maxlength=\"12\"/>\n                    <input v-model=\"user.password\" type=\"password\" placeholder=\"Password\" />\n                    <p v-if=\"user.password.length < 6 && user.password.length > 1\" class=\"text-red-500 font-semibold py-2 text-sm\">Mật khẩu phải ít nhất 6 kí tự</p>\n                    <input v-model=\"user.repeatPassword\" type=\"password\" placeholder=\"Repeat your password\" />\n                    <p v-if=\"user.password !== user.repeatPassword\" class=\"text-red-500 font-semibold py-2 text-sm\">Mật khẩu nhập lại không chính xác!</p>\n\n                    <button type=\"submit\">Register</button>\n                </form>\n            </div>\n            <div class=\"overlay-container\">\n                <div class=\"overlay\">\n                    <div class=\"overlay-panel overlay-right\">\n                        <h1>Register Form</h1>\n                        <p>Have already an account? Login now!</p>\n                        <a href=\"/login\">\n                            <button>Login</button>\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </body>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n    data() {\n        return {\n            user : {\n                userName: '',\n                fullName: '',\n                phone: '',\n                password: '',\n                repeatPassword: '',\n                checkPasswordLength: true\n            },\n        }\n    },\n    methods: {\n        async register() {\n            // Kiểm tra xem mật khẩu có trùng khớp không\n            if (this.user.password !== this.user.repeatPassword) {\n                alert('Mật khẩu nhập lại không chính xác');\n                return;\n            }\n\n            if(this.user.password.length < 6){\n                this.user.checkPasswordLength = false;\n                return;\n            }\n            try {\n                // Gửi yêu cầu POST đến API Spring Boot để đăng ký\n                const response = await axios.post('http://localhost:8086/auth/register', {\n                    userName: this.user.userName,\n                    password: this.user.password,\n                    fullName: this.user.fullName,\n                    phone: this.user.phone,\n                });\n\n                // Xử lý dữ liệu nhận được từ API (response.data) nếu cần\n                console.log('Đăng ký tài khoản thành công', response.data);\n\n                // Hiển thị thông báo và chuyển hướng sau khi đăng ký thành công\n                alert('Đăng ký tài khoản thành công');\n                this.$router.push('/login'); // Chuyển hướng đến trang login\n            } catch (error) {\n                // Xử lý lỗi từ API (error.response.data) nếu cần\n                console.error('Registration failed', error.response.data);\n            }\n        },\n    },\n};\n</script>\n"],"mappings":";AAkCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAG,EAAI;QACHC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,cAAc,EAAE,EAAE;QAClBC,mBAAmB,EAAE;MACzB;IACJ;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,QAAQA,CAAA,EAAG;MACb;MACA,IAAI,IAAI,CAACR,IAAI,CAACI,QAAO,KAAM,IAAI,CAACJ,IAAI,CAACK,cAAc,EAAE;QACjDI,KAAK,CAAC,mCAAmC,CAAC;QAC1C;MACJ;MAEA,IAAG,IAAI,CAACT,IAAI,CAACI,QAAQ,CAACM,MAAK,GAAI,CAAC,EAAC;QAC7B,IAAI,CAACV,IAAI,CAACM,mBAAkB,GAAI,KAAK;QACrC;MACJ;MACA,IAAI;QACA;QACA,MAAMK,QAAO,GAAI,MAAMb,KAAK,CAACc,