{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Cookies from 'js-cookie';\nimport axios from 'axios';\nimport { findApiByName } from '../../../assets/js/apiUtil.js';\n// import App from '../../../App.vue';\n\nexport default {\n  name: 'HeaderScreen',\n  data() {\n    return {\n      isAuthenticated: false,\n      showDropdown: false,\n      role: ''\n    };\n  },\n  mounted() {\n    this.checkCurrentRoute();\n  },\n  watch: {\n    $route() {\n      // Gọi phương thức kiểm tra khi có sự thay đổi trong route\n      this.checkCurrentRoute();\n      this.role = Cookies.get(\"role\");\n      this.showDropdown = false;\n    }\n  },\n  methods: {\n    checkCurrentRoute() {\n      const currentPath = this.$route.path;\n      const token = Cookies.get(\"auth\");\n      console.log(currentPath);\n      // Kiểm tra nếu đường dẫn là '/home'\n      if (currentPath !== '/login' && token) {\n        this.isAuthenticated = true;\n        // Thực hiện các hành động bạn muốn khi ở trang Home\n      }\n      console.log(this.isAuthenticated);\n    },\n    toggleDropdown() {\n      this.showDropdown = !this.showDropdown;\n    },\n    async logout() {\n      try {\n        const token = Cookies.get(\"auth\");\n        const formData = new FormData();\n        formData.append('token', token);\n        const apiObject = findApiByName(\"auth\", \"logout\").url;\n        await axios.post(apiObject, formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        }).then(res => {\n          if (res.status === 200) {\n            this.deleteCookie(\"auth\");\n            this.deleteCookie(\"userID\");\n            this.deleteCookie(\"role\");\n            this.$router.push('/login');\n          }\n        });\n      } catch (error) {\n        console.error('Get infor failed', error.response.data);\n      }\n    },\n    deleteCookie(name) {\n      document.cookie = `${name}=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;`;\n    }\n  }\n};","map":{"version":3,"names":["Cookies","axios","findApiByName","name","data","isAuthenticated","showDropdown","role","mounted","checkCurrentRoute","watch","$route","get","methods","currentPath","path","token","console","log","toggleDropdown","logout","formData","FormData","append","apiObject","url","post","headers","then","res","status","deleteCookie","$router","push","error","response","document","cookie"],"sources":["C:\\Vuejs\\Elearning\\src\\components\\client\\common\\HeaderScreen.vue"],"sourcesContent":["<template>\r\n  <header\r\n    class=\"bg-white z-10 fixed top-0 flex items-center justify-between p-5 h-16 shadow-md shadow-gray-300 w-full left-0 right-0\">\r\n    <div class=\"flex items-center\">\r\n      <div class=\"header__logo\">\r\n        <img src=\"../../../assets/images/client/logo.png\" alt=\"\">\r\n      </div>\r\n      <form action=\"\" class=\"relative border rounded-md px-2 flex items-center\">\r\n        <input type=\"text\" placeholder=\"Tìm khóa học, chuyên gia...\" class=\"left-0 outline-none h-full py-2 text-sm w-56\">\r\n        <label for=\"search__submit\" class=\"absolute right-3 cursor-pointer\">\r\n          <i class=\"fa-solid fa-magnifying-glass text-gray-400\"></i>\r\n        </label>\r\n        <input type=\"submit\" id=\"search__submit\" hidden value=\"tìm\">\r\n      </form>\r\n    </div>\r\n\r\n    <div class=\"header__middle\">\r\n      <ul class=\"flex justify-between\">\r\n        <li><a href=\"/home\" class=\"navigate__link mr-8\">Trang chủ</a></li>\r\n        <li><a href=\"/training\" class=\"navigate__link mr-8\">Đào tạo</a></li>\r\n        <li><a href=\"/course\" class=\"navigate__link mr-8\">Khóa học</a></li>\r\n        <li><a href=\"/support\" class=\"navigate__link mr-8\">Hỗ trợ</a></li>\r\n      </ul>\r\n    </div>\r\n\r\n    <div class=\"\">\r\n      <div v-if=\"isAuthenticated\" class=\"\">\r\n        <div class=\"flex items-center gap-4 \">\r\n          <div class=\"\">\r\n            <a v-if=\"role === 'USER'\" href=\"/my-course\"\r\n              class=\"font-medium border border-blue-900 text-blue-900  py-1 px-4 rounded-md transition-all hover:bg-blue-900 hover:text-white\">Học\r\n              tập</a>\r\n            <a v-if=\"role === 'EXPERT'\" href=\"/my-course\"\r\n              class=\"font-medium border border-blue-900 text-blue-900  py-1 px-4 rounded-md transition-all hover:bg-blue-900 hover:text-white\">Quản\r\n              lý</a>\r\n          </div>\r\n          <div class=\"relative\">\r\n            <img @click=\"toggleDropdown\" class=\"w-8 h-8 rounded-full cursor-pointer\"\r\n              src=\"../../../assets/images/client/avatars/avatar.png\" alt=\"\">\r\n            <ul v-if=\"showDropdown\" class=\"absolute right-0 mt-2 bg-white border rounded-md shadow-md w-48 p-4\">\r\n              <li class=\"border-b border-gray-300 mb-2\"><a class=\"p-2\"\r\n                  :href=\"role === 'USER' ? '/profile' : (role === 'EXPERT' ? '/profile-expert' : '#')\">Thông tin tài\r\n                  khoản</a></li>\r\n              <li class=\"border-b border-gray-300 mb-2\"><a class=\"p-2\" href=\"/changepass\">Đổi mật khẩu</a></li>\r\n              <li><a class=\"p-2\" @click=\"logout\" href=\"#\">Đăng xuất</a></li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n\r\n      </div>\r\n\r\n      <div v-else class=\"flex items-center gap-4\">\r\n        <a href=\"/login\"\r\n          class=\"font-medium border border-blue-900 text-blue-900  py-1 px-4 rounded-md transition-all hover:bg-blue-900 hover:text-white\">Đăng\r\n          nhập</a>\r\n        <a href=\"/register\"\r\n          class=\"font-medium border border-blue-900 text-blue-900  py-1 px-4 rounded-md transition-all hover:bg-blue-900 hover:text-white\">Đăng\r\n          ký</a>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nimport Cookies from 'js-cookie';\r\nimport axios from 'axios';\r\nimport { findApiByName } from '../../../assets/js/apiUtil.js';\r\n// import App from '../../../App.vue';\r\n\r\nexport default {\r\n  name: 'HeaderScreen',\r\n  data() {\r\n    return {\r\n      isAuthenticated: false,\r\n      showDropdown: false,\r\n      role: ''\r\n    };\r\n  },\r\n\r\n\r\n  mounted() {\r\n\r\n    this.checkCurrentRoute();\r\n  },\r\n  watch: {\r\n    $route() {\r\n      // Gọi phương thức kiểm tra khi có sự thay đổi trong route\r\n      this.checkCurrentRoute();\r\n      this.role = Cookies.get(\"role\");\r\n      this.showDropdown = false;\r\n    },\r\n  },\r\n\r\n  methods: {\r\n\r\n    checkCurrentRoute() {\r\n      const currentPath = this.$route.path;\r\n      const token = Cookies.get(\"auth\");\r\n      console.log(currentPath);\r\n      // Kiểm tra nếu đường dẫn là '/home'\r\n      if (currentPath !== '/login' && token) {\r\n        this.isAuthenticated = true;\r\n        // Thực hiện các hành động bạn muốn khi ở trang Home\r\n      }\r\n      console.log(this.isAuthenticated);\r\n    },\r\n\r\n    toggleDropdown() {\r\n      this.showDropdown = !this.showDropdown;\r\n    },\r\n\r\n\r\n    async logout() {\r\n      try {\r\n        const token = Cookies.get(\"auth\");\r\n        const formData = new FormData();\r\n        formData.append('token', token);\r\n        const apiObject = findApiByName(\"auth\", \"logout\").url;\r\n        await axios.post(apiObject, formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data',\r\n          },\r\n        }).then((res) => {\r\n          if (res.status === 200) {\r\n            this.deleteCookie(\"auth\");\r\n            this.deleteCookie(\"userID\");\r\n            this.deleteCookie(\"role\");\r\n            this.$router.push('/login');\r\n          }\r\n        })\r\n\r\n      } catch (error) {\r\n        console.error('Get infor failed', error.response.data);\r\n      }\r\n    },\r\n\r\n    deleteCookie(name) {\r\n      document.cookie = `${name}=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;`;\r\n    },\r\n  },\r\n\r\n\r\n}\r\n</script>\r\n"],"mappings":";AAgEA,OAAOA,OAAM,MAAO,WAAW;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,aAAY,QAAS,+BAA+B;AAC7D;;AAEA,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,KAAK;MACtBC,YAAY,EAAE,KAAK;MACnBC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EAGDC,OAAOA,CAAA,EAAG;IAER,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,KAAK,EAAE;IACLC,MAAMA,CAAA,EAAG;MACP;MACA,IAAI,CAACF,iBAAiB,CAAC,CAAC;MACxB,IAAI,CAACF,IAAG,GAAIP,OAAO,CAACY,GAAG,CAAC,MAAM,CAAC;MAC/B,IAAI,CAACN,YAAW,GAAI,KAAK;IAC3B;EACF,CAAC;EAEDO,OAAO,EAAE;IAEPJ,iBAAiBA,CAAA,EAAG;MAClB,MAAMK,WAAU,GAAI,IAAI,CAACH,MAAM,CAACI,IAAI;MACpC,MAAMC,KAAI,GAAIhB,OAAO,CAACY,GAAG,CAAC,MAAM,CAAC;MACjCK,OAAO,CAACC,GAAG,CAACJ,WAAW,CAAC;MACxB;MACA,IAAIA,WAAU,KAAM,QAAO,IAAKE,KAAK,EAAE;QACrC,IAAI,CAACX,eAAc,GAAI,IAAI;QAC3B;MACF;MACAY,OAAO,CAACC,GAAG,CAAC,IAAI,CAACb,eAAe,CAAC;IACnC,CAAC;IAEDc,cAAcA,CAAA,EAAG;MACf,IAAI,CAACb,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC,CAAC;IAGD,MAAMc,MAAMA,CAAA,EAAG;MACb,IAAI;QACF,MAAMJ,KAAI,GAAIhB,OAAO,CAACY,GAAG,CAAC,MAAM,CAAC;QACjC,MAAMS,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;QAC/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAEP,KAAK,CAAC;QAC/B,MAAMQ,SAAQ,GAAItB,aAAa,CAAC,MAAM,EAAE,QAAQ,CAAC,CAACuB,GAAG;QACrD,MAAMxB,KAAK,CAACyB,IAAI,CAACF,SAAS,EAAEH,QAAQ,EAAE;UACpCM,OAAO,EAAE;YACP,cAAc,EAAE;UAClB;QACF,CAAC,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;UACf,IAAIA,GAAG,CAACC,MAAK,KAAM,GAAG,EAAE;YACtB,IAAI,CAACC,YAAY,CAAC,MAAM,CAAC;YACzB,IAAI,CAACA,YAAY,CAAC,QAAQ,CAAC;YAC3B,IAAI,CAACA,YAAY,CAAC,MAAM,CAAC;YACzB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;UAC7B;QACF,CAAC;MAEH,EAAE,OAAOC,KAAK,EAAE;QACdjB,OAAO,CAACiB,KAAK,CAAC,kBAAkB,EAAEA,KAAK,CAACC,QAAQ,CAAC/B,IAAI,CAAC;MACxD;IACF,CAAC;IAED2B,YAAYA,CAAC5B,IAAI,EAAE;MACjBiC,QAAQ,CAACC,MAAK,GAAK,GAAElC,IAAK,mDAAkD;IAC9E;EACF;AAGF"},"metadata":{},"sourceType":"module","externalDependencies":[]}