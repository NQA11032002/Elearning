{"ast":null,"code":"import HomeScreen from \"./components/client/HomeScreen.vue\";\nimport HeaderScreen from \"./components/client/HeaderScreen.vue\";\nimport FooterScreen from \"./components/client/FooterScreen.vue\";\nimport TrainingScreen from \"./components/client/TrainingScreen.vue\";\nimport SupportScreen from \"./components/client/SupportScreen.vue\";\nimport CourseScreen from \"./components/client/CourseScreen.vue\";\nimport LoginScreen from \"./components/client/LoginScreen.vue\";\nimport RegisterScreen from \"./components/client/RegisterScreen.vue\";\nimport { ref } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  setup() {\n    const courses = ref([]);\n    const page = ref(0);\n    const records = 4;\n    console.log(page);\n    const getAllCourses = async () => {\n      try {\n        const res = await axios.get(\"http://localhost:8080/api/course\", {\n          params: {\n            page: page,\n            records: records\n          }\n        });\n        courses.value = res.data.data;\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    const showMore = newPage => {\n      page.value = newPage;\n    };\n    getAllCourses();\n    return {\n      courses,\n      showMore\n    };\n  },\n  name: 'App',\n  components: {\n    HomeScreen,\n    TrainingScreen,\n    CourseScreen,\n    SupportScreen,\n    HeaderScreen,\n    FooterScreen,\n    LoginScreen,\n    RegisterScreen\n  },\n  data() {\n    return {\n      page: \"course\"\n    };\n  }\n};","map":{"version":3,"names":["HomeScreen","HeaderScreen","FooterScreen","TrainingScreen","SupportScreen","CourseScreen","LoginScreen","RegisterScreen","ref","axios","setup","courses","page","records","console","log","getAllCourses","res","get","params","value","data","error","showMore","newPage","name","components"],"sources":["F:\\VueJS\\thaylam\\Elearning\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <header-screen v-if=\"page!='login' && page!='register'\"></header-screen>\n    <home-screen v-if=\"page === 'home'\" :courses=\"courses\"></home-screen>\n    <training-screen v-if=\"page === 'training'\"></training-screen>\n    <support-screen v-if=\"page === 'support'\"></support-screen>\n    <course-screen v-if=\"page === 'course'\" :courses=\"courses\" @showMore=\"showMore\"></course-screen>\n    <login-screen v-if=\"page === 'login'\"></login-screen>\n    <register-screen v-if=\"page === 'register'\"></register-screen>\n    <footer-screen v-if=\"page!='login' && page!='register'\"></footer-screen>\n  </div>\n</template>\n\n<script>\n  import HomeScreen from \"./components/client/HomeScreen.vue\";\n  import HeaderScreen from \"./components/client/HeaderScreen.vue\";\n  import FooterScreen from \"./components/client/FooterScreen.vue\";\n  import TrainingScreen from \"./components/client/TrainingScreen.vue\";\n  import SupportScreen from \"./components/client/SupportScreen.vue\";\n  import CourseScreen from \"./components/client/CourseScreen.vue\";\n  import LoginScreen from \"./components/client/LoginScreen.vue\";\n  import RegisterScreen from \"./components/client/RegisterScreen.vue\";\n  import { ref } from \"vue\";\n  import axios from \"axios\";\n\n\nexport default {\n  setup(){\n    const courses = ref([]);\n\n    const page = ref(0);\n    const records = 4;\n    console.log(page);\n    const getAllCourses = async () => {\n      try\n      {\n        const res = await axios.get(\"http://localhost:8080/api/course\", {\n          params: {\n            page: page,\n            records: records\n          }\n        });\n\n        courses.value = res.data.data;\n\n      } catch (error) {\n        console.log(error)\n      }\n    }\n\n    const showMore = (newPage) => {\n      page.value = newPage;\n    }\n\n    getAllCourses();\n\n    return {\n      courses,\n      showMore\n    }\n  },\n  name: 'App',\n  components: {\n    HomeScreen,\n    TrainingScreen,\n    CourseScreen,\n    SupportScreen,\n    HeaderScreen,\n    FooterScreen,\n    LoginScreen,\n    RegisterScreen\n  },\n  data(){\n    return{\n      page: \"course\",\n    };\n  }\n}\n</script>\n"],"mappings":"AAcE,OAAOA,UAAS,MAAO,oCAAoC;AAC3D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,OAAOC,aAAY,MAAO,uCAAuC;AACjE,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,WAAU,MAAO,qCAAqC;AAC7D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,SAASC,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAG3B,eAAe;EACbC,KAAKA,CAAA,EAAE;IACL,MAAMC,OAAM,GAAIH,GAAG,CAAC,EAAE,CAAC;IAEvB,MAAMI,IAAG,GAAIJ,GAAG,CAAC,CAAC,CAAC;IACnB,MAAMK,OAAM,GAAI,CAAC;IACjBC,OAAO,CAACC,GAAG,CAACH,IAAI,CAAC;IACjB,MAAMI,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IACA;QACE,MAAMC,GAAE,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,kCAAkC,EAAE;UAC9DC,MAAM,EAAE;YACNP,IAAI,EAAEA,IAAI;YACVC,OAAO,EAAEA;UACX;QACF,CAAC,CAAC;QAEFF,OAAO,CAACS,KAAI,GAAIH,GAAG,CAACI,IAAI,CAACA,IAAI;MAE/B,EAAE,OAAOC,KAAK,EAAE;QACdR,OAAO,CAACC,GAAG,CAACO,KAAK;MACnB;IACF;IAEA,MAAMC,QAAO,GAAKC,OAAO,IAAK;MAC5BZ,IAAI,CAACQ,KAAI,GAAII,OAAO;IACtB;IAEAR,aAAa,CAAC,CAAC;IAEf,OAAO;MACLL,OAAO;MACPY;IACF;EACF,CAAC;EACDE,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACV1B,UAAU;IACVG,cAAc;IACdE,YAAY;IACZD,aAAa;IACbH,YAAY;IACZC,YAAY;IACZI,WAAW;IACXC;EACF,CAAC;EACDc,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJT,IAAI,EAAE;IACR,CAAC;EACH;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}