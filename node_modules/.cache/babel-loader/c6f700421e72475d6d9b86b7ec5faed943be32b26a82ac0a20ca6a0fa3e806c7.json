{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      user: {\n        userName: '',\n        fullName: '',\n        phone: '',\n        password: '',\n        repeatPassword: '',\n        checkPasswordLength: true\n      }\n    };\n  },\n  methods: {\n    async register() {\n      // Kiểm tra xem mật khẩu có trùng khớp không\n      if (this.user.password !== this.user.repeatPassword) {\n        alert('Mật khẩu nhập lại không chính xác');\n        return;\n      }\n      if (this.user.password.length < 6) {\n        this.user.checkPasswordLength = false;\n        return;\n      }\n      try {\n        // Gửi yêu cầu POST đến API Spring Boot để đăng ký\n        const response = await axios.post('http://localhost:8086/auth/register', {\n          userName: this.user.userName,\n          password: this.user.password,\n          fullName: this.user.fullName,\n          phone: this.user.phone\n        });\n\n        // Xử lý dữ liệu nhận được từ API (response.data) nếu cần\n        console.log('Đăng ký tài khoản thành công', response.data);\n\n        // Hiển thị thông báo và chuyển hướng sau khi đăng ký thành công\n        alert('Đăng ký tài khoản thành công');\n        this.$router.push('/login'); // Chuyển hướng đến trang login\n      } catch (error) {\n        // Xử lý lỗi từ API (error.response.data) nếu cần\n        console.error('Registration failed', error.response.data);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","user","userName","fullName","phone","password","repeatPassword","checkPasswordLength","methods","register","alert","length","response","post","console","log","$router","push","error"],"sources":["C:\\Vuejs\\Elearning\\src\\components\\client\\RegisterScreen.vue"],"sourcesContent":["<template>\r\n    <body class=\"LoginForm\">\r\n        <div class=\"LoginForm_container\" id=\"container\">\r\n            <div class=\"form-container log-in-container\">\r\n                <form @submit.prevent=\"register\">\r\n                    <h1>Create Account</h1>\r\n                    <span>Create your new account</span>\r\n                    <input v-model=\"user.userName\" type=\"text\" placeholder=\"Username\" />\r\n                    <input v-model=\"user.fullName\" type=\"text\" placeholder=\"Full name\" />\r\n                    <input v-model=\"user.phone\" type=\"text\" placeholder=\"Phone number\" maxlength=\"12\"/>\r\n                    <input v-model=\"user.password\" type=\"password\" placeholder=\"Password\" />\r\n                    <p v-if=\"user.password.length < 6 && user.password.length > 1\" class=\"text-red-500 font-semibold py-2 text-sm\">Mật khẩu phải ít nhất 6 kí tự</p>\r\n                    <input v-model=\"user.repeatPassword\" type=\"password\" placeholder=\"Repeat your password\" />\r\n                    <p v-if=\"user.password !== user.repeatPassword\" class=\"text-red-500 font-semibold py-2 text-sm\">Mật khẩu nhập lại không chính xác!</p>\r\n\r\n                    <button type=\"submit\">Register</button>\r\n                </form>\r\n            </div>\r\n            <div class=\"overlay-container\">\r\n                <div class=\"overlay\">\r\n                    <div class=\"overlay-panel overlay-right\">\r\n                        <h1>Register Form</h1>\r\n                        <p>Have already an account? Login now!</p>\r\n                        <a href=\"/login\">\r\n                            <button>Login</button>\r\n                        </a>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </body>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            user : {\r\n                userName: '',\r\n                fullName: '',\r\n                phone: '',\r\n                password: '',\r\n                repeatPassword: '',\r\n                checkPasswordLength: true\r\n            },\r\n        }\r\n    },\r\n    methods: {\r\n        async register() {\r\n            // Kiểm tra xem mật khẩu có trùng khớp không\r\n            if (this.user.password !== this.user.repeatPassword) {\r\n                alert('Mật khẩu nhập lại không chính xác');\r\n                return;\r\n            }\r\n\r\n            if(this.user.password.length < 6){\r\n                this.user.checkPasswordLength = false;\r\n                return;\r\n            }\r\n            try {\r\n                // Gửi yêu cầu POST đến API Spring Boot để đăng ký\r\n                const response = await axios.post('http://localhost:8086/auth/register', {\r\n                    userName: this.user.userName,\r\n                    password: this.user.password,\r\n                    fullName: this.user.fullName,\r\n                    phone: this.user.phone,\r\n                });\r\n\r\n                // Xử lý dữ liệu nhận được từ API (response.data) nếu cần\r\n                console.log('Đăng ký tài khoản thành công', response.data);\r\n\r\n                // Hiển thị thông báo và chuyển hướng sau khi đăng ký thành công\r\n                alert('Đăng ký tài khoản thành công');\r\n                this.$router.push('/login'); // Chuyển hướng đến trang login\r\n            } catch (error) {\r\n                // Xử lý lỗi từ API (error.response.data) nếu cần\r\n                console.error('Registration failed', error.response.data);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n"],"mappings":";AAkCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAG,EAAI;QACHC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,cAAc,EAAE,EAAE;QAClBC,mBAAmB,EAAE;MACzB;IACJ;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,QAAQA,CAAA,EAAG;MACb;MACA,IAAI,IAAI,CAACR,IAAI,CAACI,QAAO,KAAM,IAAI,CAACJ,IAAI,CAACK,cAAc,EAAE;QACjDI,KAAK,CAAC,mCAAmC,CAAC;QAC1C;MACJ;MAEA,IAAG,IAAI,CAACT,IAA