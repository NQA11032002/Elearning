eatPassword) {\n        alert('Mật khẩu nhập lại không chính xác');\n        return;\n      }\n\n      if (this.user.password.length < 6) {\n        this.user.checkPasswordLength = false;\n        return;\n      }\n      try {\n        const apiObject = findApiByName(\"auth\", \"register\").url;\n\n        const response = await axios.post(apiObject, {\n          userName: this.user.userName,\n          password: this.user.password,\n          fullName: this.user.fullName,\n          phone: this.user.phone,\n        });\n\n        // Xử lý dữ liệu nhận được từ API (response.data) nếu cần\n        console.log('Đăng ký tài khoản thành công', response.data);\n\n        // Hiển thị thông báo và chuyển hướng sau khi đăng ký thành công\n        alert('Đăng ký tài khoản thành công');\n        this.$router.push('/login'); // Chuyển hướng đến trang login\n      } catch (error) {\n        // Xử lý lỗi từ API (error.response.data) nếu cần\n        console.error('Registration failed', error.response.data);\n      }\n    },\n  },\n};\n</script>\n"],"mappings":";AAoCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,aAAY,QAAS,+BAA+B;AAC7D,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,cAAc,EAAE,EAAE;QAClBC,mBAAmB,EAAE;MACvB;IAEF;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf;MACA,IAAI,IAAI,CAACR,IAAI,CAACI,QAAO,KAAM,IAAI,CAACJ,IAAI,CAACK,cAAc,EAAE;QACnDI,KAAK,CAAC,mCAAmC,CAAC;QAC1C;MACF;MAEA,IAAI,IAAI,CAACT,IAAI,CAACI,QAAQ,CAACM,MAAK,GAAI,CAAC,EAAE;QACjC,IAAI,CAACV,IAAI,CAACM,mBAAkB,GAAI,KAAK;QACrC;MACF;MACA,IAAI;QACF,MAAMK,SAAQ,GAAIb,aAAa,CAAC,MAAM,EAAE,UAAU,CAAC,CAACc,GAAG;QAEvD,MAAMC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,IAAI,CAACH,SAAS,EAAE;UAC3CV,QAAQ,EAAE,IAAI,CAACD,IAAI,CAACC,QAAQ;UAC5BG,QAAQ,EAAE,IAAI,CAACJ,IAAI,CAACI,QAAQ;UAC5BF,QAAQ,EAAE,IAAI,CAACF,IAAI,CAACE,QAAQ;UAC5BC,KAAK,EAAE,IAAI,CAACH,IAAI,CAACG;QACnB,CAAC,CAAC;;QAEF;QACAY,OAAO,CAACC,GAAG,CAAC,8BAA8B,EAAEH,QAAQ,CAACd,IAAI,CAAC;;QAE1