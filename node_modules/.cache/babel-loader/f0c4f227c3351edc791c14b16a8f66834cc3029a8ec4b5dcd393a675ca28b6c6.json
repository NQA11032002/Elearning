);\n\n        courses.value = res.data.data;\n        totalPage.value = res.data.totalPage;\n        \n        // Phát ra sự kiện để thông báo danh sách khóa học mới cho component con\n        emit(\"coursesChanged\", courses.value);\n\n      } catch (error) {\n        console.log(error)\n      }\n    }\n\n    const onPageChanged  = (newPage) => {\n      page.value = newPage;\n      getAllCourses();\n    }\n\n    getAllCourses();\n\n    return {\n      totalPage,\n      courses,\n      onPageChanged \n    }\n  },\n  name: 'App',\n  components: {\n    HomeScreen,\n    TrainingScreen,\n    CourseScreen,\n    SupportScreen,\n    HeaderScreen,\n    FooterScreen,\n    LoginScreen,\n    RegisterScreen\n  },\n  data(){\n    return{\n      page: \"course\",\n    };\n  }\n}\n</script>\n"],"mappings":"AAcE,OAAOA,UAAS,MAAO,oCAAoC;AAC3D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,OAAOC,aAAY,MAAO,uCAAuC;AACjE,OAAOC,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,WAAU,MAAO,qCAAqC;AAC7D,OAAOC,cAAa,MAAO,wCAAwC;AACnE,SAASC,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,OAAO;AAG3B,eAAe;EACbC,OAAMA,CAAA,EAAK,CACX,CAAC;EACDC,KAAKA,CAACC,KAAK,EAAE;IAAEC;EAAK,CAAC,EAAC;IACpB,MAAMC,OAAM,GAAIN,GAAG,CAAC,EAAE,CAAC;IACvB,MAAMO,IAAG,GAAIP,GAAG,CAAC,CAAC,CAAC;IACnB,MAAMQ,OAAM,GAAI,CAAC;IACjB,MAAMC,SAAQ,GAAIT,GAAG,CAAC,CAAC,CAAC;IAExB,MAAMU,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IACA;QACE,MAAMC,GAAE,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,kCAAkC,EAAE;UAC9DC,MAAM,EAAE;YACNN,IAAI,EAAEA,IAAI,CAACO,KAAK;YAChBN,OAAO,EAAEA;UACX;QACF,CAAC,CAAC;QAEFF,OAAO,CAACQ,KAAI,GAAIH,GAAG,CAACI,IAAI,CAACA,IAAI;QAC7BN,SAAS,CAACK,KAAI,GAAIH,GAAG,CAACI,IAAI,CAACN,SAAS;;QAEpC;QACAJ,IAAI,CAAC,gBAAgB,EAAEC,OAAO,CAACQ,KAAK,CAAC;MAEvC,EAAE,OAAOE,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB;IACF;IAEA,MAAMG,aAAY,GAAMC,OAAO,IAAK;MAClCb,IAAI,CAACO,KAAI,GAAIM,OAAO;MACpBV,aAAa,CAAC,CAAC;IACjB;IAEAA,aAAa,CAAC,CAAC;IAEf,OAAO;MACLD,SAAS;MACTH,OAAO;MACPa;IACF;EACF,CAAC;EACDE,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACV9B,UAAU;IACVG,cAAc;